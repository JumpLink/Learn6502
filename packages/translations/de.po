msgid "<b>W</b> or Up Arrow key (<tt>$77</tt>)"
msgstr "<b>W</b> oder Pfeil nach oben (<tt>$77</tt>)"

#: ../learn/dist/quick-help.ui:18
msgid "<b>A</b> or Left Arrow key (<tt>$61</tt>)"
msgstr "<b>A</b> oder Pfeil nach links (<tt>$61</tt>)"

