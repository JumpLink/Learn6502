<Page xmlns="http://schemas.nativescript.org/tns.xsd">
    <ActionBar title="Material Design 3 Colors" />
    <ScrollView>
        <StackLayout class="p-10">
            <Label text="Primary Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box primary-color m-r-5 m-b-5">
                    <Label text="Primary" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-primary-color m-l-5 m-b-5">
                    <Label text="On Primary" class="color-label" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box primary-container-color m-r-5 m-t-5">
                    <Label text="Primary Container" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box on-primary-container-color m-l-5 m-t-5">
                    <Label text="On Primary Container" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Secondary Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box secondary-color m-r-5 m-b-5">
                    <Label text="Secondary" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-secondary-color m-l-5 m-b-5">
                    <Label text="On Secondary" class="color-label" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box secondary-container-color m-r-5 m-t-5">
                    <Label text="Secondary Container" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box on-secondary-container-color m-l-5 m-t-5">
                    <Label text="On Secondary Container" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Tertiary Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box tertiary-color m-r-5 m-b-5">
                    <Label text="Tertiary" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-tertiary-color m-l-5 m-b-5">
                    <Label text="On Tertiary" class="color-label" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box tertiary-container-color m-r-5 m-t-5">
                    <Label text="Tertiary Container" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box on-tertiary-container-color m-l-5 m-t-5">
                    <Label text="On Tertiary Container" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Error Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box error-color m-r-5 m-b-5">
                    <Label text="Error" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-error-color m-l-5 m-b-5">
                    <Label text="On Error" class="color-label" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box error-container-color m-r-5 m-t-5">
                    <Label text="Error Container" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box on-error-container-color m-l-5 m-t-5">
                    <Label text="On Error Container" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Surface Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box surface-color m-r-5 m-b-5">
                    <Label text="Surface" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-surface-color m-l-5 m-b-5">
                    <Label text="On Surface" class="color-label" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box surface-variant-color m-r-5 m-t-5">
                    <Label text="Surface Variant" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box on-surface-variant-color m-l-5 m-t-5">
                    <Label text="On Surface Variant" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Surface Container Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto, auto, auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box surface-container-color m-r-5 m-b-5">
                    <Label text="Surface Container" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box surface-container-lowest-color m-l-5 m-b-5">
                    <Label text="Surface Container Lowest" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="0" row="1" class="color-box surface-container-low-color m-r-5 m-t-5 m-b-5">
                    <Label text="Surface Container Low" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="1" class="color-box surface-container-high-color m-l-5 m-t-5 m-b-5">
                    <Label text="Surface Container High" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="0" row="2" class="color-box surface-container-highest-color m-r-5 m-t-5">
                    <Label text="Surface Container Highest" class="color-label" textWrap="true" />
                </StackLayout>
                <StackLayout col="1" row="2" class="color-box surface-bright-color m-l-5 m-t-5">
                    <Label text="Surface Bright" class="color-label" textWrap="true" />
                </StackLayout>
            </GridLayout>

            <Label text="Background Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box background-color m-r-5">
                    <Label text="Background" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box on-background-color m-l-5">
                    <Label text="On Background" class="color-label" />
                </StackLayout>
            </GridLayout>

            <Label text="Outline Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box outline-color m-r-5">
                    <Label text="Outline" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box outline-variant-color m-l-5">
                    <Label text="Outline Variant" class="color-label" />
                </StackLayout>
            </GridLayout>

            <Label text="Fixed Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box primary-fixed-color m-r-5">
                    <Label text="Primary Fixed" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box primary-fixed-dim-color m-l-5">
                    <Label text="Primary Fixed Dim" class="color-label" />
                </StackLayout>
            </GridLayout>

            <Label text="Inverse Colors" class="h2 m-b-10" />
            <GridLayout columns="*, *" rows="auto" class="m-b-20">
                <StackLayout col="0" row="0" class="color-box surface-inverse-color m-r-5">
                    <Label text="Inverse Surface" class="color-label" />
                </StackLayout>
                <StackLayout col="1" row="0" class="color-box primary-inverse-color m-l-5">
                    <Label text="Inverse Primary" class="color-label" />
                </StackLayout>
            </GridLayout>
    </StackLayout>
    </ScrollView>
</Page>